{
  "version": "2",
  "plugins": [
    {
      "name": "ts",
      "process": {
        "cmd": "./bin/sqlc-sql.js-gen"
      }
    }
  ],
   "sql": [
    {
      "schema": "schema.sql",
      "queries": "query.sql",
      "engine": "sqlite",
      "codegen": [
        {
          "out": "gen",
          "plugin": "ts"
        }
      ]
    }
  ]
}